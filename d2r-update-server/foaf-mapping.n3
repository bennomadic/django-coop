map:database a d2rq:Database;
	d2rq:jdbcDriver "org.postgresql.Driver";
	d2rq:jdbcDSN "jdbc:postgresql://127.0.0.1/base/";
	d2rq:username "dom";
	d2rq:password "q4s5d6";
	d2rq:textColumn "coop_local_membre.telephone_fixe";
	.
# Table coop_local_membre
map:membre a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "membre/@@coop_local_membre.id@@";
	d2rq:class foaf:Person;
	d2rq:classDefinitionLabel "Membre";
	.
	
#   d2rq:additionalClassDefinitionProperty map:PersonEquivalence
#   .
# 
# map:PersonEquivalence a d2rq:AdditionalProperty;
#   d2rq:propertyName owl:equivalentClass;
#   d2rq:propertyValue org:Member
#   .   

map:membre__label a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:membre;
	d2rq:property rdfs:label;
	d2rq:pattern "@@coop_local_membre.prenom@@ @@coop_local_membre.nom@@";
	.	
	
map:membre_nom a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:membre;
	d2rq:property foaf:familyName;
	d2rq:propertyDefinitionLabel "Nom";
	d2rq:column "coop_local_membre.nom";
	.
map:membre_prenom a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:membre;
	d2rq:property foaf:givenName;
	d2rq:propertyDefinitionLabel "Prenom";
	d2rq:column "coop_local_membre.prenom";
	.
map:membre_ville a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:membre;
	d2rq:property foaf:based_near;
	d2rq:propertyDefinitionLabel "Localisation";
	d2rq:column "coop_local_membre.ville";
	.
map:membre_telephone_fixe a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:membre;
	d2rq:property foaf:phone;
	d2rq:propertyDefinitionLabel "Telephone";
	d2rq:column "coop_local_membre.telephone_fixe";
	d2rq:condition "coop_local_membre.telephone_fixe <> ''"
	.
map:membre_email_sha1 a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:membre;
	d2rq:property foaf:mbox_sha1sum;
	d2rq:propertyDefinitionLabel "E-mail";
	d2rq:column "coop_local_membre.email";
	d2rq:condition "coop_local_membre.email <> ''"
	.

